{
  "name": "ui-next",
  "version": "1.0.1",
  "description": "This UI for shortlink application",
  "repository": "https://github.com/shortlink-org/shortlink.git",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "build": "npx next build",
    "start": "npx ts-node --project tsconfig.server.json server/server.ts",
    "start:dev": "cp .env.dev .env.local && npm start",
    "start:k8s": "cp .env.k8s .env.local && npm start",
    "export": "NODE_ENV=production npx next export && npx next-export-optimize-images",
    "type-check": "tsc",
    "next-lint": "npx next lint",
    "prettier": "npx prettier -w -c .",
    "lint": "npx eslint ./",
    "lint:fix": "npx eslint --fix ./",
    "swagger": "npx swagger-typescript-api -p ../../api/swagger.json -o ./store/api -d -r --modular --axios",
    "sitemap": "npx next-sitemap --config config/sitemap.config.js",
    "cypress": "NODE_ENV=test npx cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "e2e": "NODE_ENV=test start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "NODE_ENV=test start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "keywords": [],
  "author": "Victor Login <batazor111@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shortlink-org/shortlink/issues/new?template=bug_report.md",
    "email": "batazor111@gmail.com"
  },
  "dependencies": {
    "@emotion/css": "^11.11.0",
    "@heroicons/react": "2.0.18",
    "@material-table/core": "6.1.15",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.4",
    "@mui/styled-engine-sc": "5.12.0",
    "@mui/styles": "5.13.2",
    "@ory/client": "1.1.33",
    "@ory/integrations": "^1.1.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/nextjs": "7.54.0",
    "@shortlink-org/ui-kit": "1.3.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "0.4.4",
    "clsx": "^1.2.1",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "firebase": "9.22.1",
    "js-cookie": "^3.0.5",
    "jspdf-autotable": "3.5.29",
    "localforage": "1.10.0",
    "lodash": "^4.17.21",
    "next": "^13.4.4",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.0.0",
    "next-themes": "^0.2.1",
    "prop-types": "^15.8.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-redux": "^8.0.7",
    "react-toastify": "^9.1.3",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.2.3",
    "styled-components": "^5.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "2.3.0",
    "@next/eslint-plugin-next": "13.4.4",
    "@redux-devtools/extension": "^3.2.5",
    "@svgr/webpack": "8.0.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/express": "^4.17.17",
    "@types/lodash": "^4.14.195",
    "@types/material-ui": "^0.21.12",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "cypress": "12.13.0",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "7.32.2",
    "next-export-optimize-images": "2.1.0",
    "next-sitemap": "4.1.3",
    "prettier": "2.8.8",
    "sharp": "^0.32.1",
    "tailwindcss": "^3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "webpack-shell-plugin": "^0.5.0"
  }
}
