{
  "name": "ui-next",
  "version": "1.0.0",
  "description": "This UI for shortlink application",
  "repository": "https://github.com/batazor/shortlink.git",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "build": "$(npm bin)/next build",
    "start": "$(npm bin)/ts-node --project tsconfig.server.json server/server.ts",
    "start:dev": "cp .env.dev .env.local && npm start",
    "start:k8s": "cp .env.k8s .env.local && npm start",
    "generate": "NODE_ENV=production $(npm bin)/next build && $(npm bin)/next export",
    "type-check": "tsc",
    "next-lint": "$(npm bin)/next lint",
    "prettier": "$(npm bin)/prettier -w -c .",
    "lint": "$(npm bin)/eslint ./",
    "lint:fix": "$(npm bin)/eslint --fix ./",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "swagger": "npx swagger-typescript-api -p ../../api/swagger.json -o ./store/api -d -r --modular --axios",
    "sitemap": "$(npm bin)/next-sitemap --config config/sitemap.config.js",
    "cypress": "NODE_ENV=test $(npm bin)/cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "e2e": "NODE_ENV=test start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "NODE_ENV=test start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "keywords": [],
  "author": "Victor Login <batazor111@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/batazor/shortlink/issues/new?template=bug_report.md",
    "email": "batazor111@gmail.com"
  },
