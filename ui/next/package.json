{
  "name": "ui-next",
  "version": "1.0.0",
  "description": "This UI for shortlink application",
  "repository": "https://github.com/batazor/shortlink.git",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server/server.ts",
    "build": "$(npm bin)/next build",
    "start": "API_URI=http://api-shortlink-api.shortlink npm run dev",
    "cloud": "API_URI_PORT=$(kubectl -n nginx-ingress get svc --field-selector \"metadata.name=nginx-ingress-ingress-nginx-controller\" -o json | jq '(.items|select([0])) | first.spec.ports | .[] | select(.name == \"https\") | .nodePort') API_URI=https://shortlink.local ts-node server/server.ts",
    "generate": "NODE_ENV=production $(npm bin)/next build && $(npm bin)/next export",
    "type-check": "tsc",
    "next-lint": "$(npm bin)/next lint",
    "prettier": "$(npm bin)/prettier -w -c .",
    "lint": "$(npm bin)/eslint ./",
    "lint:fix": "$(npm bin)/eslint --fix ./"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "keywords": [],
  "author": "Victor Login <batazor111@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/batazor/shortlink/issues/new?template=bug_report.md",
    "email": "batazor111@gmail.com"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.0",
    "@heroicons/react": "^1.0.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@ory/kratos-client": "^0.6.3-alpha.1",
    "@sentry/nextjs": "^6.11.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.23.0",
    "express": "^4.17.1",
    "js-cookie": "^3.0.0",
    "jspdf-autotable": "^3.5.15",
    "lodash": "^4.17.21",
    "material-table": "^1.63.0",
    "next": "^11.0.1",
    "next-redux-wrapper": "^7.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-undraw-illustrations": "^2.0.3",
    "recharts": "^2.0.10",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-saga": "^1.1.3",
    "tailwindcss": "^2.2.7",
    "uuid": "^8.3.2",
    "webpack": "^5.50.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^11.0.1",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.172",
    "@types/material-ui": "^0.21.9",
    "@types/node": "^16.4.1",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "@zeit/next-source-maps": "0.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "2.3.2",
    "sharp": "^0.28.3",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "webpack-shell-plugin": "^0.5.0"
  }
}
