{
  "name": "ui-next",
  "version": "1.0.0",
  "description": "",
  "repository": "https://github.com/batazor/shortlink.git",
  "main": "server/index.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server/server.ts",
    "build": "$(npm bin)/next build",
    "start": "API_URI=http://api-shortlink-api.shortlink npm run dev",
    "cloud": "API_URI_PORT=$(kubectl -n nginx-ingress get svc --field-selector \"metadata.name=nginx-ingress-ingress-nginx-controller\" -o json | jq '(.items|select([0])) | first.spec.ports | .[] | select(.name == \"https\") | .nodePort') API_URI=https://shortlink.local ts-node server/server.ts",
    "generate": "NODE_ENV=production $(npm bin)/next build && $(npm bin)/next export",
    "type-check": "tsc",
    "lint": "$(npm bin)/eslint ./src",
    "lint:fix": "$(npm bin)/eslint --fix ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "keywords": [],
  "author": "Victor Login <batazor111@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^1.3.0",
    "@heroicons/react": "^1.0.1",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "@ory/kratos-client": "^0.6.3-alpha.1",
    "@sentry/nextjs": "^6.8.0",
    "@tailwindcss/aspect-ratio": "^0.2.1",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "clsx": "^1.1.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.22.1",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "jspdf-autotable": "^3.5.14",
    "material-table": "^1.69.3",
    "next": "^11.0.0",
    "next-redux-wrapper": "^7.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-undraw-illustrations": "^2.0.3",
