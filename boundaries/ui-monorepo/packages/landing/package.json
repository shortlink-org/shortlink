    "eslint-config-shortlink": "workspace:^",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.0.5",
    "tailwindcss": "3.4.13",
    "typescript": "^5.6.2"
  },
  "devDependencies": {
    "@mdx-js/loader": "3.0.1",
