{
  "name": "ui-next",
  "version": "1.0.1",
  "description": "This UI for shortlink application",
  "repository": "https://github.com/shortlink-org/shortlink.git",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production pnpm next build --turbopack",
    "start": "NODE_OPTIONS='--max-http-header-size=131072' pnpm next dev",
    "start:dev": "cp .env.dev .env.local && pnpm start",
    "start:k8s": "cp .env.k8s .env.local && pnpm start",
    "start:prod": "cp .env.prod .env.local && pnpm start",
    "type-check": "tsc",
    "next-lint": "pnpm dlx next lint",
    "prettier": "pnpm dlx prettier -w -c .",
    "inspector": "pnpm dlx @eslint/config-inspector",
    "lint": "pnpm dlx eslint ./ --max-warnings 0 --report-unused-disable-directives --cache",
    "lint:fix": "pnpm dlx eslint --fix",
    "lint:gitlab": "pnpm dlx lint --format gitlab",
    "swagger": "pnpm dlx swagger-typescript-api -p ../../api/swagger.json -o ./store/api -d -r --modular --axios",
    "sitemap": "pnpm dlx next-sitemap --config config/sitemap.config.js",
    "cypress": "NODE_ENV=test pnpm dlx cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "playwright:codegen": "playwright codegen http://localhost:3000/add-link",
    "playwright:report": "allure serve playwright-report/allure-results",
    "preinstall": "npx only-allow pnpm"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && pnpm lint"
    }
  },
  "keywords": [],
  "author": "Victor Login <batazor111@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shortlink-org/shortlink/issues/new?template=bug_report.md",
    "email": "batazor111@gmail.com"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "2.2.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/material-nextjs": "^7.3.6",
    "@mui/x-date-pickers": "^8.21.0",
    "@next/env": "^16.0.8",
    "@ory/client": "^1.22.15",
    "@ory/integrations": "^1.3.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@sentry/nextjs": "^10.29.0",
    "@shortlink-org/eslint-plugin-shortlink": "0.20250809.193608",
    "@shortlink-org/ui-kit": "^0.20251206.220856",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "axios": "^1.13.2",
    "babel-plugin-react-compiler": "^1.0.0",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "express": "5.2.1",
    "firebase": "^12.6.0",
    "js-cookie": "^3.0.5",
    "jspdf-autotable": "^5.0.2",
    "localforage": "1.10.0",
    "lodash": "^4.17.21",
    "next": "^16.0.8",
    "next-themes": "^0.4.6",
    "prop-types": "^15.8.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-redux": "^9.2.0",
    "react-toastify": "^11.0.5",
    "react-wrap-balancer": "^1.1.1",
    "redux": "^5.0.1",
    "redux-saga": "^1.4.2",
    "schema-dts": "^1.1.5",
    "shortlink-ui-kit": "^1.4.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@mdx-js/loader": "3.1.1",
    "@playwright/test": "^1.57.0",
    "@redux-devtools/extension": "^3.3.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/express": "^5.0.6",
    "@types/lodash": "^4.17.21",
    "@types/material-ui": "^0.21.18",
    "@types/node": "24.10.2",
    "@types/react": "19.2.7",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "allure-playwright": "^3.4.3",
    "autoprefixer": "^10.4.23",
    "cypress": "^15.7.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "next-sitemap": "^4.2.3",
    "patch-package": "^8.0.1",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "pnpm@10.26.2+sha512.0e308ff2005fc7410366f154f625f6631ab2b16b1d2e70238444dd6ae9d630a8482d92a451144debc492416896ed16f7b114a86ec68b8404b2443869e68ffda6"
}
