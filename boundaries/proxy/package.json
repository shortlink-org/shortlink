  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@prisma/client": "7.1.0",
    "@testcontainers/rabbitmq": "^11.9.0",
    "@testcontainers/redis": "^11.9.0",
    "@types/amqplib": "0.10.8",
    "@types/compression": "1.8.1",
    "@types/node": "^24.10.2",
    "@types/swagger-jsdoc": "6.0.4",
    "@typescript-eslint/eslint-plugin": "8.48.1",
