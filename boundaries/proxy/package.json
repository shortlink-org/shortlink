  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@prisma/client": "7.4.2",
    "@testcontainers/rabbitmq": "^11.12.0",
    "@testcontainers/redis": "^11.12.0",
    "@types/amqplib": "0.10.8",
    "@types/compression": "1.8.1",
    "@types/node": "^24.10.9",
    "@types/swagger-jsdoc": "6.0.4",
    "@typescript-eslint/eslint-plugin": "8.53.1",
