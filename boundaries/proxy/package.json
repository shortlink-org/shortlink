{
  "name": "proxy",
  "version": "1.0.0",
  "type": "module",
  "description": "Proxy service for redirect to original URL",
  "main": "./dist/app.js",
  "exports": {
    ".": "./dist/app.js"
  },
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "pnpm run build && NODE_ENV=development node --permission --allow-fs-read=$(pwd) --allow-fs-read=/app --allow-net ./dist/app.js",
    "prod": "NODE_ENV=production node --permission --allow-fs-read=$(pwd) --allow-fs-read=/app --allow-net ./dist/app.js",
    "start:dev": "pnpm run build && NODE_ENV=development node --permission --allow-fs-read=$(pwd) --allow-fs-read=/app --allow-net ./dist/app.js",
    "start:permissive": "pnpm run build && NODE_ENV=development node ./dist/app.js",
    "prod:permissive": "NODE_ENV=production node ./dist/app.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest run --coverage",
    "docs:generate": "tsx src/interfaces/http/swagger/generate-openapi.ts",
    "migrate:deploy": "pnpm exec prisma migrate deploy --schema=./prisma/schema.prisma",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "shortlink",
    "example",
    "microservice",
    "architecture"
  ],
  "author": "@batazor",
  "license": "MIT",
  "engines": {
    "node": ">=25.0.0"
  },
  "dependencies": {
    "@bufbuild/protobuf": "2.10.1",
    "@connectrpc/connect": "2.1.0",
    "@connectrpc/connect-node": "2.1.0",
    "@godaddy/terminus": "^4.12.1",
    "@opentelemetry/api": "1.9.0",
    "@opentelemetry/auto-instrumentations-node": "0.67.0",
    "@opentelemetry/exporter-prometheus": "0.208.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "0.208.0",
    "@opentelemetry/instrumentation": "0.208.0",
    "@opentelemetry/resources": "2.2.0",
    "@opentelemetry/sdk-metrics": "2.2.0",
    "@opentelemetry/sdk-node": "0.208.0",
    "@opentelemetry/sdk-trace-base": "2.2.0",
    "@opentelemetry/sdk-trace-node": "2.2.0",
    "@opentelemetry/semantic-conventions": "1.38.0",
    "@pyroscope/nodejs": "0.4.7",
    "@types/express": "^5.0.5",
    "@types/morgan": "1.9.10",
    "amqplib": "^0.10.3",
    "body-parser": "2.2.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "inversify": "^7.10.4",
    "inversify-express-utils": "^6.5.0",
    "inversify-logger-middleware": "^3.1.0",
    "ioredis": "^5.4.1",
    "morgan": "1.10.1",
    "neverthrow": "8.2.0",
    "reflect-metadata": "^0.2.2",
    "swagger-jsdoc": "6.2.8",
    "winston": "3.18.3",
    "zod": "4.1.12",
    "zod-validation-error": "5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@prisma/client": "6.19.0",
    "@types/body-parser": "1.19.6",
    "@types/node": "^24.10.1",
    "@types/supertest": "6.0.3",
    "@types/swagger-jsdoc": "6.0.4",
    "@typescript-eslint/eslint-plugin": "8.46.4",
    "@typescript-eslint/parser": "8.46.4",
    "@testcontainers/rabbitmq": "^11.8.1",
    "@testcontainers/redis": "^11.8.1",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "9.39.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-node": "11.1.0",
    "globals": "16.5.0",
    "prisma": "6.19.0",
    "supertest": "7.1.4",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
