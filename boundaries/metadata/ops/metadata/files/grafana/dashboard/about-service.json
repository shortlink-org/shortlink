        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.95, sum by (le) (rate(grpc_server_handling_seconds_bucket{job=~\"$job\",instance=~\"$instance\",grpc_service=~\"$grpc_service\",grpc_method=~\"$grpc_method\"}[$__rate_interval])))",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "decimals": 4, "min": 0 },
        "overrides": []
