    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 4 },
      "id": 10,
      "type": "stat",
      "title": "gRPC RPS (handled)",
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(grpc_server_handled_total{job=~\"$job\",instance=~\"$instance\",grpc_service=~\"$grpc_service\",grpc_method=~\"$grpc_method\"}[$__rate_interval]))",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps", "decimals": 2 },
        "overrides": []
      }
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 4 },
      "id": 11,
      "type": "stat",
      "title": "gRPC Error % (non-OK)",
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * (sum(rate(grpc_server_handled_total{job=~\"$job\",instance=~\"$instance\",grpc_service=~\"$grpc_service\",grpc_method=~\"$grpc_method\",grpc_code!=\"OK\"}[$__rate_interval])) / clamp_min(sum(rate(grpc_server_handled_total{job=~\"$job\",instance=~\"$instance\",grpc_service=~\"$grpc_service\",grpc_method=~\"$grpc_method\"}[$__rate_interval])), 1e-9))",
          "range": true
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent", "decimals": 2, "min": 0 },
        "overrides": []
      }
    },
    {
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 4 },
      "id": 12,
      "type": "stat",
      "title": "gRPC P95 Latency",
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
