{
  "private": true,
  "engines": {
    "node": ">=23.9.0",
    "pnpm": ">=10.6.3"
  },
  "scripts": {
    "dev": "pnpm next dev",
    "start": "NODE_ENV=production pnpm next start",
    "start:dev": "cp .env.dev .env && pnpm dev",
    "start:prod": "cp .env.prod .env && pnpm dev",
    "build": "NODE_ENV=production pnpm next build",
    "prettier": "pnpm dlx prettier --write --ignore-unknown .",
    "prettier:check": "pnpm dlx prettier --check --ignore-unknown .",
    "test": "pnpm dlx pnpm prettier:check"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@types/react-dom": "^19.0.4",
    "clsx": "^2.1.1",
    "geist": "^1.3.1",
    "next": "15.2.2",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "sonner": "^2.0.1"
  },
  "devDependencies": {
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.14",
    "typescript": "5.8.2"
  }
}
