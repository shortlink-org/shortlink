{
  "name": "ui-next",
  "version": "1.0.1",
  "description": "This UI for shortlink application",
  "repository": "https://github.com/shortlink-org/shortlink.git",
  "private": true,
  "main": "server/index.js",
  "scripts": {
    "build": "NODE_ENV=production npx next build",
    "start": "NODE_OPTIONS='--max-http-header-size=131072' npx next dev",
    "start:dev": "cp .env.dev .env.local && npm start",
    "start:k8s": "cp .env.k8s .env.local && npm start",
    "start:prod": "cp .env.prod .env.local && npm start",
    "type-check": "tsc",
    "next-lint": "npx next lint",
    "prettier": "npx prettier -w -c .",
    "lint": "npx eslint ./ --max-warnings 0 --report-unused-disable-directives --cache",
    "lint:fix": "npm run lint -- --fix",
    "lint:gitlab": "npm run lint -- --format gitlab",
    "swagger": "npx swagger-typescript-api -p ../../api/swagger.json -o ./store/api -d -r --modular --axios",
    "sitemap": "npx next-sitemap --config config/sitemap.config.js",
    "cypress": "NODE_ENV=test npx cypress open",
    "cypress:headless": "NODE_ENV=test cypress run",
    "e2e": "NODE_ENV=test start-server-and-test start http://localhost:3000 cypress",
    "e2e:headless": "NODE_ENV=test start-server-and-test start http://localhost:3000 cypress:headless"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint"
    }
  },
  "keywords": [],
  "author": "Victor Login <batazor111@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/shortlink-org/shortlink/issues/new?template=bug_report.md",
    "email": "batazor111@gmail.com"
  },
  "dependencies": {
    "@heroicons/react": "2.1.1",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.15.4",
    "@ory/client": "^1.5.1",
    "@ory/integrations": "^1.1.5",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/nextjs": "^7.93.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "0.4.4",
    "@shortlink-org/ui-kit": "*",
    "eslint-config-shortlink": "*",
    "clsx": "^2.1.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^3.2.0",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "js-cookie": "^3.0.5",
    "jspdf-autotable": "^3.8.1",
    "localforage": "1.10.0",
    "lodash": "^4.17.21",
    "next-redux-wrapper": "^8.1.0",
    "next-seo": "^6.4.0",
    "next-themes": "^0.2.1",
    "prop-types": "^15.8.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-redux": "^9.0.4",
    "react-toastify": "^9.1.3",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@mdx-js/loader": "3.0.0",
    "@redux-devtools/extension": "^3.3.0",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/lodash": "^4.14.202",
    "@types/material-ui": "^0.21.16",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "cypress": "^13.6.2",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "next-sitemap": "^4.2.3",
    "prettier": "^3.1.1",
    "sharp": "^0.33.2",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack-shell-plugin": "^0.5.0"
  },
  "nx": {
    "tags": [
      "scope:ui-next"
    ]
  }
}
